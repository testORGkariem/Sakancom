<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sakancom.SignUp">
   <children>
      <TextField fx:id="Name" layoutX="106.0" layoutY="69.0" promptText="Full name" style="-fx-border-color: black;" />
      <TextField fx:id="UserName" layoutX="106.0" layoutY="112.0" promptText="User Name" style="-fx-border-color: black;" />
      <TextField fx:id="Email" layoutX="106.0" layoutY="161.0" promptText="Email" style="-fx-border-color: black;" />
      <TextField fx:id="Age" layoutX="106.0" layoutY="301.0" promptText="Age" style="-fx-border-color: black;" />
      <Button layoutX="159.0" layoutY="365.0" mnemonicParsing="false" onAction="#signUp" text="Signup" />
      <Hyperlink layoutX="118.0" layoutY="401.0" onAction="#backToLogin" text="already have account?" />
      <ChoiceBox fx:id="Role" layoutX="106.0" layoutY="334.0" prefWidth="150.0" style="-fx-border-color: black;" />
      <PasswordField fx:id="Password" layoutX="106.0" layoutY="206.0" promptText="Password" style="-fx-border-color: black;" />
      <PasswordField fx:id="RePassword" layoutX="107.0" layoutY="253.0" promptText="Re Password" style="-fx-border-color: black;" />
      <TextField fx:id="Phone" layoutX="106.0" layoutY="31.0" promptText="Phone Number" style="-fx-border-color: black;" />
   </children>
</AnchorPane>
